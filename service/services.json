{
	"id": "info.mobo.moboreader.service",
	"description": "Article handling for MoboReader",
	"engine": "node",
	"activityTimeout": 30,
	"services": [
		{
			"name": "info.mobo.moboreader.service",
			"description": "Article handling for MoboReader",
			"globalized": false,
			"assistant": "ServiceAssistant",
			"commands": [
				{
					"name": "storeArticleContent",
					"assistant": "StoreArticleContentAssistant",
					"public": true
				},
				{
					"name": "getArticleContent",
					"assistant": "GetArticleContentAssistant",
					"public": true
				},
				{
					"name": "articleContentExists",
					"assistant": "ArticleContentExistsAssistant",
					"public": true
				},
				{
					"name": "deleteArticleContent",
					"assistant": "DeleteArticleContentAssistant",
					"public": true
				},
				{
					"name": "wipeStorage",
					"assistant": "WipeStorageAssistant",
					"public": true
				},
				{
					"name": "downloadSpritz",
					"assistant": "DownloadSpritzAssistant",
					"public": true
				}
			]
		}
	]
}
